# tmux-like pane controls
'atom-workspace':
  'ctrl-b %': 'pane:split-right-and-move-active-item'
  'ctrl-b "': 'pane:split-down-and-move-active-item'
  'ctrl-b x': 'pane:close'
  'ctrl-b up': 'window:focus-pane-above'
  'ctrl-b down': 'window:focus-pane-below'
  'ctrl-b left': 'window:focus-pane-on-left'
  'ctrl-b right': 'window:focus-pane-on-right'
  'ctrl-b shift-up': 'window:move-active-item-to-pane-above'
  'ctrl-b shift-down': 'window:move-active-item-to-pane-below'
  'ctrl-b shift-left': 'window:move-active-item-to-pane-on-left'
  'ctrl-b shift-right': 'window:move-active-item-to-pane-on-right'
